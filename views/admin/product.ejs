<%- include('header') %>

<% if(messages && messages.length>0){%>
    <div class="hilirio alert">
        <div class=" alertmag m-0" role="alert">
            <strong>
                <%= messages %>
            </strong>
        </div>
    </div>
    <%} %>
    
    
            <!-- Container-fluid starts-->
            <div class="page-body">
                <!-- All User Table Start -->
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card card-table">
                                <div class="card-body">
                                    <div class="title-header option-title">
                                        <h5>All Product</h5>
                                        <form class="d-inline-flex">
                                            <a href="/addproduct" class="align-items-center btn btn-theme d-flex">
                                                <i data-feather="plus-square"></i>Add New
                                            </a>
                                        </form>
                                    </div>

                                    <div class="table-responsive category-table">
                                        <div>
                                            <table class="table all-package theme-table" id="table_id">
                                                <thead>
                                                    <tr>
                                                        <th>Sr no.</th>
                                                        <th>Category Name</th>
                                                        <th>Subcategory Name</th>
                                                        <th>Product Name</th>
                                                        <th>Current Price</th>
                                                        <th>Original Price</th>
                                                        <th>Product Image</th>
                                                        <th>Option</th>
                                                    </tr>
                                                </thead>

                                                <tbody>


                                                    <% product.map((val, i)=>{ i=i+1%>
                                                        <tr>
                                                            <td>
                                                                <%= i %>
                                                            </td>
                                                            <td>
                                                                <%= val.cat_id.cat_name %>
                                                            </td>

                                                            <td>
                                                                <%= val.sub_id.subcat_name %>
                                                            </td>

                                                            <td>
                                                                <%= val.pro_name %>
                                                            </td>

                                                            <td>
                                                                <%= val.pro_cprice %>
                                                            </td>

                                                            <td>
                                                                <%= val.pro_oprice %>
                                                            </td>

                                                            <td>
                                                                <div class="table-image">
                                                                    <% val.pro_Thumb.forEach((img)=>{%>
                                                                        <img src="<%= img %>" alt="">
                                                                    <%}) %>
                                                                </div>
                                                            </td>

                                                            

                                                            

                                                            <td>
                                                                <ul>
                                                                    <li>
                                                                        <a href="order-detail.html">
                                                                            <i class="ri-eye-line"></i>
                                                                        </a>
                                                                    </li>

                                                                    <li>
                                                                        <a href="javascript:void(0)">
                                                                            <i class="ri-pencil-line"></i>
                                                                        </a>
                                                                    </li>

                                                                    <li>
                                                                        <a href="/deleteData?id=<%= val.id %>">
                                                                            <i class="ri-delete-bin-line"></i>
                                                                        </a>
                                                                        <!-- <a href="/deleteData?id=<%= //val.id %>"
                                                                            data-bs-toggle="modal"
                                                                            data-bs-target="#exampleModalToggle">
                                                                            <i class="ri-delete-bin-line"></i>
                                                                        </a> -->
                                                                    </li>
                                                                </ul>
                                                            </td>
                                                        </tr>
                                                        <!-- Delete Modal Box Start -->
                                                        <div class="modal fade theme-modal remove-coupon"
                                                            id="exampleModalToggle" aria-hidden="true" tabindex="-1">
                                                            <div class="modal-dialog modal-dialog-centered">
                                                                <div class="modal-content">
                                                                    <div class="modal-header d-block text-center">
                                                                        <h5 class="modal-title w-100"
                                                                            id="exampleModalLabel22">Are You Sure ?</h5>
                                                                        <button type="button" class="btn-close"
                                                                            data-bs-dismiss="modal" aria-label="Close">
                                                                            <i class="fas fa-times"></i>
                                                                        </button>
                                                                    </div>
                                                                    <div class="modal-body">
                                                                        <div class="remove-box">
                                                                            <p>The permission for the use/group, preview
                                                                                is inherited from the object, object
                                                                                will create a new permission for this
                                                                                object</p>
                                                                        </div>
                                                                    </div>
                                                                    <div class="modal-footer">
                                                                        <button type="button"
                                                                            class="btn btn-animation btn-md fw-bold"
                                                                            data-bs-dismiss="modal">No</button>
                                                                        <button type="button"
                                                                            class="btn btn-animation btn-md fw-bold"
                                                                            data-bs-target="#exampleModalToggle2"
                                                                            data-bs-toggle="modal"
                                                                            data-bs-dismiss="modal">Yes</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="modal fade theme-modal remove-coupon"
                                                            id="exampleModalToggle2" aria-hidden="true" tabindex="-1">
                                                            <div class="modal-dialog modal-dialog-centered">
                                                                <div class="modal-content">
                                                                    <div class="modal-header">
                                                                        <h5 class="modal-title text-center"
                                                                            id="exampleModalLabel12">Done!</h5>
                                                                        <button type="button" class="btn-close"
                                                                            data-bs-dismiss="modal" aria-label="Close">
                                                                            <i class="fas fa-times"></i>
                                                                        </button>
                                                                    </div>
                                                                    <div class="modal-body">
                                                                        <div class="remove-box text-center">
                                                                            <div class="wrapper">
                                                                                <svg class="checkmark"
                                                                                    xmlns="http://www.w3.org/2000/svg"
                                                                                    viewBox="0 0 52 52">
                                                                                    <circle class="checkmark__circle"
                                                                                        cx="26" cy="26" r="25"
                                                                                        fill="none" />
                                                                                    <path class="checkmark__check"
                                                                                        fill="none"
                                                                                        d="M14.1 27.2l7.1 7.2 16.7-16.8" />
                                                                                </svg>
                                                                            </div>
                                                                            <h4 class="text-content">It's Removed.</h4>
                                                                        </div>
                                                                    </div>
                                                                    <div class="modal-footer">
                                                                        <button class="btn btn-primary"
                                                                            data-bs-toggle="modal"
                                                                            data-bs-dismiss="modal">Close</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- Delete Modal Box End -->
                                                        <% }) %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- All User Table Ends-->

                <div class="container-fluid">
                    <!-- footer start-->
                    <footer class="footer">
                        <div class="row">
                            <div class="col-md-12 footer-copyright text-center">
                                <p class="mb-0">Copyright 2022 Â© Fastkart theme by pixelstrap</p>
                            </div>
                        </div>
                    </footer>
                    <!-- footer end-->
                </div>
            </div>
            <!-- Container-fluid end -->
        </div>
        <!-- Page Body End -->

        <!-- Modal Start -->
        <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1">
            <div class="modal-dialog  modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-body">
                        <h5 class="modal-title" id="staticBackdropLabel">Logging Out</h5>
                        <p>Are you sure you want to log out?</p>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        <div class="button-box">
                            <button type="button" class="btn btn--no" data-bs-dismiss="modal">No</button>
                            <button type="button" class="btn  btn--yes btn-primary">Yes</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Modal End -->
    </div>



    <!-- latest js -->
    <script src="/public/admin assets/js/jquery-3.6.0.min.js"></script>

    <!-- Bootstrap js -->
    <script src="/public/admin assets/js/bootstrap/bootstrap.bundle.min.js"></script>

    <!-- feather icon js -->
    <script src="/public/admin assets/js/icons/feather-icon/feather.min.js"></script>
    <script src="/public/admin assets/js/icons/feather-icon/feather-icon.js"></script>

    <!-- scrollbar simplebar js -->
    <script src="/public/admin assets/js/scrollbar/simplebar.js"></script>
    <script src="/public/admin assets/js/scrollbar/custom.js"></script>

    <!-- customizer js -->
    <script src="/public/admin assets/js/customizer.js"></script>

    <!-- Sidebar js -->
    <script src="/public/admin assets/js/config.js"></script>

    <!-- Plugins JS -->
    <script src="/public/admin assets/js/sidebar-menu.js"></script>
    <!-- <script src="/public/admin assets/js/notify/bootstrap-notify.min.js"></script> -->
    <script src="/public/admin assets/js/notify/index.js"></script>

    <!-- Data table js -->
    <script src="/public/admin assets/js/jquery.dataTables.js"></script>
    <script src="/public/admin assets/js/custom-data-table.js"></script>

    <!-- sidebar effect -->
    <script src="/public/admin assets/js/sidebareffect.js"></script>

    <!-- Theme js -->
    <script src="/public/admin assets/js/script.js"></script>
</body>

</html>